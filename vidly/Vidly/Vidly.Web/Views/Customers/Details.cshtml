@model Vidly.Data.Models.Customer

@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2>@Model.Name</h2>
<ul>
    <li>Membership Type: @Model.MembershipType.Name</li>
    @if (Model.Dob != null)
    {
        <li>Birthdate: @Model.Dob.ToShortDateString()</li>
    }
</ul>
